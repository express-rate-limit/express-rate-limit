{
	"name": "express-rate-limit-example-stores",
	"version": "1.0.0",
	"description": "A minimal example project using external stores for the express-rate-limit package.",
	"type": "module",
	"scripts": {
		"test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --forceExit"
	},
	"dependencies": {
		"@types/rate-limit-redis": "^1.7.4",
		"express": "^4.18.2",
		"express-rate-limit": "file:../../..",
		"precise-memory-rate-limit": "^1.1.4",
		"rate-limit-memcached": "^0.6.0",
		"rate-limit-mongo": "^2.3.2",
		"rate-limit-redis": "^3.0.2",
		"redis": "^4.6.7"
	},
	"devDependencies": {
		"@types/express": "^4.17.17",
		"@types/node": "^20.4.0",
		"@types/supertest": "^2.0.12",
		"cross-env": "^7.0.3",
		"jest": "^29.6.0",
		"supertest": "^6.3.3",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.1.6"
	},
	"jest": {
		"verbose": true,
		"preset": "ts-jest/presets/default-esm",
		"globals": {
			"ts-jest": {
				"useESM": true
			}
		},
		"collectCoverage": true,
		"testTimeout": 30000,
		"testMatch": [
			"**/test/**/*-test.[jt]s"
		],
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"moduleNameMapper": {
			"^(\\.{1,2}/.*)\\.js$": "$1"
		}
	}
}
