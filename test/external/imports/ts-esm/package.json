{
	"name": "express-rate-limit-example-ts-cjs",
	"version": "1.0.0",
	"description": "A minimal example (TS-CJS) of a project using the express-rate-limit package.",
	"type": "module",
	"scripts": {
		"start": "ts-node source/index.ts",
		"test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest"
	},
	"dependencies": {
		"express": "^4.17.2",
		"express-rate-limit": "file:../../../../express-rate-limit-6.0.1.tgz"
	},
	"devDependencies": {
		"@types/express": "^4.17.13",
		"@types/jest": "^27.0.3",
		"@types/node": "^17.0.4",
		"@types/supertest": "^2.0.11",
		"cross-env": "^7.0.3",
		"jest": "^27.4.5",
		"supertest": "^6.1.6",
		"ts-jest": "^27.1.2",
		"ts-node": "^10.4.0",
		"typescript": "^4.5.4"
	},
	"jest": {
		"verbose": true,
		"preset": "ts-jest/presets/default-esm",
		"globals": {
			"ts-jest": {
				"useESM": true
			}
		},
		"collectCoverage": true,
		"testTimeout": 30000,
		"testMatch": [
			"**/test/**/*-test.[jt]s"
		],
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"moduleNameMapper": {
			"^(\\.{1,2}/.*)\\.js$": "$1"
		}
	}
}
